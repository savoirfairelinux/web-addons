<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="less_helpers" name="web_enterprise less helpers" inherit_id="web_enterprise.less_helpers" priority='15'>
        <xpath expr="//link[@href='/web_enterprise/static/src/less/variables.less']" position="replace">
            <link rel="stylesheet" type="text/less" href="/ui_color_help/static/src/less/variables.less"/>
        </xpath>
    </template>

    <template id="assets_frontend_color_change" inherit_id="web_enterprise.assets_backend"> 
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/ui_color_help/static/src/css/color_change.css"/>
        </xpath>
    </template>


</odoo>

